<?xml version="1.0"?>

<!-- SUMPAC Human-Powered Airplane  -->
<!-- Copyright 2019 Wayne Bragg -->

<PropertyList>

    <path>Aircraft/SUMPAC/Models/heightpole.ac</path>
    <offsets>
        <x-m>-452</x-m>
        <y-m>-150</y-m>
        <z-m>   0</z-m>
        <scale>0</scale>
        <pitch-deg>0.0</pitch-deg>
    </offsets>

    <!--  Specularity fix -->
    <animation>
        <type>material</type>
        <object-name>heightpole</object-name>
        <shininess>128</shininess>
        <specular>
            <r>1</r>
            <g>1</g>
            <b>1</b>
        </specular>
        <ambient>
            <r>1</r>
            <g>1</g>
            <b>1</b>
        </ambient>
    </animation>

    <!-- exterior effects -->
    <!-- inherits model-combined-deferred -->
    <effect>
        <inherits-from>Aircraft/SUMPAC/Models/Effects/sumpac-exterior</inherits-from>
        <object-name>heightpole</object-name>
    </effect>

 </PropertyList>
