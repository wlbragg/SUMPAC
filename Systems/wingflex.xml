<PropertyList>

    <filter>
        <name>wingflex</name>
        <type>gain</type>
        <gain>1.0</gain>
        <input>
            <expression>
                <table>
                    <property>fdm/jsbsim/aero/force/Lift_alpha</property>
                    <entry><ind>   0 </ind><dep> -0.5 </dep></entry>
                    <entry><ind> 180 </ind><dep>  1.5 </dep></entry>
                    <entry><ind> 626 </ind><dep>  2 </dep></entry>
                </table>
            </expression>
        </input>
        <output>
            <property>fdm/jsbsim/aero/force/wingflex</property>
        </output>
    </filter>

</PropertyList>
